{
	"manifest_version": 2,

	"name": "Facebook",
	"description": "This extension allows you to remove some unnecessary panels (trash) in Facebook. Open Source: ",
	"version": "2.0.1",
    "developer": {
	    "name": "Artem Trygub"
    },
	"background": {
		"scripts": ["scripts/background_v2.0.1.js"]
	},
    "permissions": [
		"tabs", "*://*.facebook.com/*"
    ],
	"browser_action": {
		"default_icon": "images/icon128.png",
		"default_title": "Facebook"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}